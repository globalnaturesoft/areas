<%
  options = nsmodule.get_options
  areas = Naturesoft::Areas::Area.where(id: options["areas"])
  menus = Naturesoft::Menus::Menu.find(options["menu_id"])
%>

<%= render "naturesoft/"+nsmodule.engine_name+"/backend/nsmodules/"+nsmodule.module_name+"/templates/"+options["style"],
  options: options,
  areas: areas.limit(6),
  menus: menus,
  nsmodule: nsmodule
%>